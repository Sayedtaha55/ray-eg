{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {},
    "outDir": "../dist",
    "rootDir": "./",
    "noEmit": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["./**/*.ts"],
  "exclude": [
    "../node_modules",
    "../dist",
    "../dist-backend",
    "../services",
    "../components",
    "../src"
  ]
}

{
  "name": "ray-test-marketplace",
  "version": "1.0.1",
  "description": "Hyper-modern multi-vendor marketplace for Egypt.",
  "main": "index.html",
  "scripts": {
    "dev": "npx vite --config vite.config.mts",
    "backend:dev": "tsx watch --tsconfig backend/tsconfig.json backend/main.ts",
    "backend:build": "tsc -p backend/tsconfig.json",
    "backend:start": "prisma db push --schema prisma/schema.prisma && node dist/main.js",
    "backend:render:start": "prisma generate --schema prisma/schema.prisma && prisma db push --schema prisma/schema.prisma && tsx --tsconfig backend/tsconfig.json backend/main.ts",
    "backend:dev:stable": "powershell -NoProfile -Command \"$env:BOOT_MODULES='auth,shop,product,gallery,media,reservation,order,offer,users,analytics,customers,notification'; $env:SHOP_IMPORTS='none'; Remove-Item Env:MINIMAL_BOOT,Env:PORT -ErrorAction SilentlyContinue; $env:PORT='4000'; tsx watch --tsconfig backend/tsconfig.json backend/main.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "backend:typecheck": "tsc -p backend/tsconfig.json --noEmit",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:push": "prisma db push --schema prisma/schema.prisma",
    "prisma:generate:postgres": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:push:postgres": "prisma db push --schema prisma/schema.postgres.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "build": "tsx scripts/compress-public-videos.ts && npx vite build --config vite.config.mts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "checks": "npm run backend:typecheck && npm run backend:build && npm run typecheck && npm run test",
    "preview": "npx vite preview",
    "start": "npm run dev",
    "postinstall": "prisma generate --schema prisma/schema.prisma",
    "vercel-build": "npm run backend:build && npm run build",
    "backend:dev:fix": "powershell -NoProfile -Command \"$env:DATABASE_URL='postgresql://ray_user:ray_password@localhost:5433/ray_marketplace?schema=public'; tsx watch --tsconfig backend/tsconfig.json backend/main.ts\""
  },
  "keywords": [
    "react",
    "marketplace",
    "egypt",
    "ai",
    "gemini",
    "pos"
  ],
  "author": "Ray Team",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.0.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.1.11",
    "@prisma/client": "^6.19.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/morgan": "^1.9.10",
    "@types/passport-jwt": "^4.0.1",
    "@vitejs/plugin-react": "^5.1.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^7.2.7",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "express-rate-limit": "^7.1.5",
    "express-winston": "^4.2.0",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.400.0",
    "morgan": "^1.10.1",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.19.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "recharts": "^3.6.0",
    "sharp": "^0.33.5",
    "tsx": "^4.20.0",
    "vite": "^7.3.1",
    "winston": "^3.19.0",
    "autoprefixer": "^10.4.23"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.11",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/leaflet": "^1.9.19",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "postcss": "^8.5.6",
    "supertest": "^7.2.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
